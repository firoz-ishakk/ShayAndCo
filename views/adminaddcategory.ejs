    <script>
      const formEl = document.querySelector("#addcategory")
      formEl.addEventListener('submit',event=>{
        if($('#categoryname').val().trim().length <= 0 ||
        $('#categorydescription').val().trim().length <= 0 ||
        $('#categoryimage').val().trim().length <= 0 )
        {
          if($('#categoryname').val().trim().length <= 0 ){
            $('#categorynameerror').html(`<p class="text-danger" style="font-size: small;">FULL NAME REQUIRED</p>`)
            $('#categoryname').css('border-color','red')
          }
          else{
            $('#categorynameerror').html('')
            $('#categoryname').css('border-color','')
          }
          if($('#categorydescription').val().trim().length <= 0 ){
            $('#categorydescriptionerror').html(`<p class="text-danger" style="font-size: small;">DESCRIPTION REQUIRED</p>`)
            $('#categorydescription').css('border-color','red')
          }
          else{
            $('#categorydescriptionerror').html('')
            $('#categorydescription').css('border-color','')
          }
          if($('#categoryimage').val().trim().length <= 0 ){
            $('#categoryimageerror').html(`<p class="text-danger" style="font-size: small;">IMAGE REQUIRED</p>`)
            $('#categoryimage').css('border-color','red')
          }
          else{
            $('#categorynameerror').html('')
            $('#categoryname').css('border-color','')
          }
        }else{
          $.ajax({
                url: '/addcategory',
                method: 'post',
                dataType: 'json',
                encode: true,
                data: $('#addcategory').serialize(),
            }).done((data) => {if (data.success) {
                    console.log(data);
                    Swal.fire({
                        position: 'top-end',
                        icon: 'success',
                        title: 'success',
                        showConfirmButton: false,
                        timer: 1500
                    })
                    setTimeout(() => {
                        window.location.reload();
                    }, 1500)
                } else {
                    console.log("ERROR! data didn't saved")
                }
            })
        }
        event.defaultPrevented()
      })

    </script>
